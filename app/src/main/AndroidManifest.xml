<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.notenotenote.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.notenotenote.app.MainViewActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.notenotenote.app.EditViewActivity"
            android:label="@string/title_activity_edit_view"
            android:parentActivityName="com.notenotenote.app.MainViewActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="MainViewActivity" />
        </activity>
    </application>

</manifest>
